<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Nao_cancelar_ordem_executada</fullName>
    <active>true</active>
    <errorConditionFormula>NOT( ISNULL(DataExecucao__c))  &amp;&amp;  ISPICKVAL(Status__c, &quot;Cancelada&quot;)</errorConditionFormula>
    <errorMessage>Não é possível cancelar uma ordem executada.</errorMessage>
</ValidationRule>
